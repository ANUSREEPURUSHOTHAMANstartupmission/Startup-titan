---
import Layout from '../layouts/Layout.astro';

import About from '../components/about.astro';

import Divc from '../components/divcomponent.astro';

import Footer from '../components/footer.astro';


---

<Layout  title="Kerala Startup Mission">
	<main class="h-full flex flex-col container flex-wrap justify-between">
	
  	<div class=" overflow-hidden absolute -z-20 bg-cover bg-center bg-no-repeat w-full h-2/3 top-0 left-0" style="background-image:url('/img/bg.jpeg')">
		<h1 class="absolute lg:text-7xl text-5xl font-bold text-white top-1/2 left-1/2 -translate-x-1/2 -translate-y-1/2 ">
			Startup Titan</h1>
	</div>  
	
	</main>
	
	<section>
		<div class="mt-96 pt-28  overflow-hidden">
			<About></About>
		</div>
	</section>

	<!-- <section>
		<div class="py-24">
			<Service></Service>
		</div>
	</section> -->

	<section>
		<div class=" container mx-auto">
			<Divc></Divc>
		</div>
	</section>

	<section>
		<div>
			<Footer></Footer>
		</div>
	</section>
</Layout>
